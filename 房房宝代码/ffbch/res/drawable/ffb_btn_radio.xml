<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_window_focused="false" android:state_checked="true" android:drawable="@color/btn_radio_on" />
    <item android:state_window_focused="false" android:state_checked="false" android:drawable="@color/btn_radio_off" />
    <item android:state_checked="true" android:state_pressed="true" android:drawable="@color/btn_radio_on_pressed" />
    <item android:state_checked="false" android:state_pressed="true" android:drawable="@color/btn_radio_off_pressed" />
    <item android:state_focused="true" android:state_checked="true" android:drawable="@color/btn_radio_on_selected" />
    <item android:state_focused="true" android:state_checked="false" android:drawable="@color/btn_radio_off_selected" />
    <item android:state_checked="false" android:drawable="@color/btn_radio_off" />
    <item android:state_checked="true" android:drawable="@color/btn_radio_on" />
</selector>